{
	"name": "core/post-title",
	"category": "design",
	"usesContext": [
		"postId",
		"postType"
	],
	"attributes": {
		"textAlign": {
			"type": "string"
		},
		"level": {
			"type": "number",
			"default": 2
		}
	},
	"supports": {
		"html": false,
		"lightBlockWrapper": true,
		"__experimentalColor": {
			"gradients": true
		},
		"__experimentalFontSize": true,
		"__experimentalLineHeight": true,
		"__experimentalSelector": {
			"attribute": "level",
			"expression": "h${attribute}",
			"range": ["h1", "h2", "h3", "h4", "h5", "h6"]
		}
	}
}
